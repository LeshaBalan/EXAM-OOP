используя  Систему ;
					
 Программа публичного класса 
{
	общедоступная  статическая  пустота  Main ()
	{
		// Пробуем с m = 100, а затем с m = 15.
		int  m  =  98 ;
		int  n  =  10 ;

		если ( m  <  100 )
   			если ( n  <  15 )
   	    {
        	Консоль . WriteLine ( " Оба зала свободны " );
   	 	}
    	еще
    	{
        	Консоль . WriteLine ( " Один зал свободен " );
    	}
	}
}
